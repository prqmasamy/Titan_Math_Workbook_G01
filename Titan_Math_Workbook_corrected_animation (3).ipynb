    "\n",
    "# Fill in all the None values below\n",
    "\n",
    "def vis_viva_speed(r, a, mu):\n",
    "    \"\"\"Speed at distance r on an orbit with semi-major axis a (Sun-centered).\"\"\"\n",
    "    import numpy as np\n",
    "    return np.sqrt(mu * (2 / r - 1 / a)) #Return the vis-viva equation based on the given parameters\n",
    "def hohmann_outer(r1, r2, mu):\n",
    "    \"\"\"Hohmann transfer from circular orbit r1 to r2.\n",
    "    Returns: a_t, e_t, t_transfer, v_p, v_a, dv1, dv2\n",
    "    \"\"\"\n",
    "    import numpy as np\n",
    "    # geometry: \n",
    "    a_t = (r1 + r2) / 2  #derived a_t from perihelion and aphelion equations accordingly\n",
    "    e_t = (r2 - r1) / (r1 + r2) #derived e_t from perihelion and aphelion equations accordingly\n",
    "    \n",
    "    # timing (half the period of the transfer ellipse)\n",
    "    t_t = np.pi * np.sqrt(a_t**3 / mu)\n",
    "    # speeds on transfer at periapsis (at r1) and apoapsis (at r2) using the vis_viva_speed function\n",
    "    v_p = vis_viva_speed(r1, a_t, mu) #input parameters for velocity of perihelion distance\n",
    "    v_a = vis_viva_speed(r2, a_t, mu)  #input parameters for velocity of aphelion distance\n",
    "    \n",
    "    # circular speeds for the inner and outer orbits using the v_circ function\n",
    "    v1_c = np.sqrt(mu / r1)\n",
    "    v2_c = np.sqrt(mu / r2)\n",
    "    \n",
    "    # delta-vs (speed changes) at start and end of transfer\n",
    "    dv1 = v_p - v1_c #velocity change between the calculated velocity from perihelion distance and the circular speed of the first orbit\n",
    "    dv2 = v2_c - v_a #velocity change between the calculated velocity from aphelion distance and the circular speed of the second orbit\n",
    "    \n",
    "    return a_t, e_t, t_t, v_p, v_a, dv1, dv2\n"
